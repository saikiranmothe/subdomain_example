<h1>Listing notaries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notaries.each do |notary| %>
      <tr>
        <td><%= notary.name %></td>
        <td><%= notary.info %></td>
        <td><%= link_to 'Show', notary %></td>
        <td><%= link_to 'Edit', edit_notary_path(notary) %></td>
        <td><%= link_to 'Destroy', notary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notary', new_notary_path %>
